{
  "name": "rollup-libkit",
  "version": "0.0.0",
  "description": "starter template for a rollup library",
  "main": "dist/index",
  "author": "sammy.nave@gmail.com",
  "license": "MIT",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "agadoo": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.3",
    "npm-run-all": "latest",
    "prettier": "^2.0.5",
    "rollup": "^2.6.1",
    "trash-cli": "^3.0.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "clean": "trash ./dist",
    "build": "yarn run clean && rollup -c",
    "build:watch": "rollup -c -w ",
    "testSite": "cd ./test-site && yarn dev",
    "dev": "run-s clean && yarn build && run-p 'build:watch' 'testSite'",
    "test": "yarn build && node test/index.mjs",
    "pretest": "yarn run build",
    "isTreeShakable": "yarn run agadoo",
    "prepublishOnly": "yarn lint && yarn run build && yarn run isTreeShakable",
    "lint": "yarn eslint ./src/**/*.*"
  }
}
